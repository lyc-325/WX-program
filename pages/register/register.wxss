.container {
  display: flex;
  flex-direction: column;
}

.container .title {
  text-align: center;
  font-size: 20px;
}

.container > .item {
  padding: 50rpx;
  display: flex;
  align-items: center;
  margin-top: 30rpx;
}

.container > .item .label {
  padding: 10rpx;
}

.container > .item input {
  border: 1px solid #eee;
}

